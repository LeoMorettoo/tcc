<p id="notice"><%= notice %></p>

<h1>Listing Nodes</h1>
<div class="container-fluid">
  <ul class="nav nav-tabs">
    <li class="active"><a href="#">Algoritimo 1</a></li>
    <li><a href="/cenario_resultado/<%= @id %>">algoritimo 2</a></li>
  </ul>
  <table>
    <thead>
      <tr>
        <th>Variable</th>
        <th>Condition</th>
      </tr>
    </thead>

    <tbody>
      <% @nodes.each do |key,value| %>
      <tr>
        <td ><%= key.to_s.humanize %></td>
        <td><%= select_tag key, options_from_collection_for_select(value,"name","name"), {prompt: "Selecione uma opção" ,class: 'node-value-selector'}  %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>
